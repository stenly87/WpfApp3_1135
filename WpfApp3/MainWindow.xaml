<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition />
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="listBox1" SelectionChanged="listSelect"/>
        <Button Content="Test Get" Grid.Row="1" Click="btnClick"/>
        <Button Content="Test Post" Grid.Row="2" Click="btnClickPost"/>
        <StackPanel Grid.Row="3">
            <DatePicker x:Name="datePicker"/>
            <TextBox x:Name="textC"/>
            <TextBox x:Name="textSummary"/>
        </StackPanel>
        <Button Content="Test Put" Grid.Row="4" Click="btnClickPut"/>
    </Grid>
</Window>
